{
"manifest_version": 2,
    "name": "Blessica",
    "description": "Adds 'ica' to words that end in 'ess'.",
    "version": "1.3.4",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_end"
        }
    ]
}
